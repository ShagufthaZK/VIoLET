sudo docker exec -i Edge-1.20 tc class add dev eth1 parent 1: classid 1:1 htb rate 5Mbps


sudo docker exec -i Edge-1.20 tc class add dev eth1 parent 1:1 classid 1:11 htb rate 5Mbit


sudo docker exec -i Edge-1.20 tc qdisc add dev eth1 parent 1:11 handle 10: netem delay 200ms


sudo docker exec -i Edge-1.20 ip route | grep default | awk '{print $3}'


