sudo docker exec -i Edge-3.12 tc qdisc add dev eth1 handle 1: root htb default 11


sudo docker exec -i Edge-3.12 tc class add dev eth1 parent 1: classid 1:1 htb rate 50Mbps


sudo docker exec -i Edge-3.12 tc class add dev eth1 parent 1:1 classid 1:11 htb rate 50Mbit


sudo docker exec -i Edge-3.12 tc qdisc add dev eth1 parent 1:11 handle 10: netem delay 40ms


sudo docker exec -i Edge-3.12 ip route | grep default | awk '{print $3}'


